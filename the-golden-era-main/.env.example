# Frontend Environment Variables
# Copy this file to .env.local and fill in the values

# Backend API URL (deployed backend service URL)
# For local development: http://localhost:8000
# For production: Your deployed backend URL (e.g., Railway, Render, Heroku)
REACT_APP_BACKEND_URL=

# Backend Environment Variables (for separate deployment)
# MongoDB connection string
MONGO_URL=

# MongoDB database name
DB_NAME=

# CORS origins (comma-separated list of allowed origins)
# Example: https://your-frontend.vercel.app,https://your-domain.com
CORS_ORIGINS=
